                          input(type="checkbox" id="room-2")
                          label(for="room-2") 2 комнаты
                          input(type="checkbox" id="room-3")
                          label(for="room-3") 3 комнаты
                          input(type="checkbox" id="room-4" checked)
                          label(for="room-4") 4 комнаты
                      .col-xs-6
                        .filter__block__options__checkbox
